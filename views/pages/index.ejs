<%- include('../template/head')-%>

    <body>
        <h2>Blog Post App</h2>

        <form id="addBlog">
            <label for="title">Title:</label><br>
            <input type="text" id="title" class="title" name="title" placeholder="My First Blog Post!"><br>
            <label for="catagory">Catagory:</label><br>
            <input type="text" id="catagory" class="catagory" name="catagory" placeholder="Travel"><br>
            <!-- <label for="body">Body:</label><br> -->
            <!-- <input type="text" id="body" name="body" placeholder="Write your blog here!"><br><br> -->
            <label for="freeform">Write your blog post here:</label>
            <br>
            <textarea id="freeform" class="body" name="freeform" rows="4" cols="50">
            </textarea><br><br>
            <input type="submit" value="Submit">
        </form>

        <div id="posts"></div>

        <!-- <script src="/js/script.js" async defer></script> -->
    </body>

